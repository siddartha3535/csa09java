import java.util.Scanner;
public class sumofdigits
{
 public static void main(String[]args)
 {
  Scanner input=new Scanner(System.in);
  System.out.print("enter a number:");
  int n=input.nextInt();
  int sum=0,r;
  while(n!=0)
  {
   r=n%10;
   sum=sum+r;
   n=n/10;
  }
   System.out.print(sum);
}
}
