import java.util.Scanner;
public class invertedfullpyramid
{
 public static void main(String[]args)
{
 Scanner input=new Scanner(System.in);
 System.out.print("enter the no of rows:");
 int n=input.nextInt();
 for(int i=n;i>=1;i++)
 {
  for(j=0;j<=n;j++)
 {
   System.out.print(" ");
}
 for(int k=0;k<=i;k++)
 {
   System.out.print("*");
}
}
 System.out.println();
}
}
