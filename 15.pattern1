import java.util.Scanner;
public class 1pattern
{
 public static void main(String[]args)
{
 Scanner input=new Scanner(System.in);
 System.out.print("enter the numbers to be printed:");
 int x=input.nextInt();
 System.out.print("max no of times to be printed:");
 int n=input.nextInt();
 for(int i=1;i<=1;i++)
{
 for(int j=1;j<=i;j++)
 {
   system.out.print(x);
}
}
 for(int i=n-i;i>=1;i--)
{
 for(int j=1;j<=i;j++)
{
 System.out.print(x);
}
}
 System.out.println();
}
}
 
